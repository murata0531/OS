   231 000013A5 B000                <1> 		mov		al, 0x00						;   AL = 0x00;
   232 000013A7 E670                <1> 		out		0x70, al						;   outp(0x70, AL);
   233 000013A9 E471                <1> 		in		al, 0x71						;   AL = inp(0x71); // 秒データ
