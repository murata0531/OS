   238 000013B3 8903                <1> 		mov		[ebx], eax						;   [dst] = 時刻;
   239                              <1> 												;   
   240 000013B5 B800000000          <1> 		mov		eax, 0							;   ret = 0; // 正常終了
   241                              <1> .10E:											; }
